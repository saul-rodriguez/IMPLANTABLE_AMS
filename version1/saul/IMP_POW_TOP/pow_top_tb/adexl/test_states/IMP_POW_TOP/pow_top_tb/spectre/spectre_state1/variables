savedComponent = (let
(tmp1 tmp2)
(unless tmp1
tmp1 = make_sevVariableStruct()
)
(unless tmp1
tmp1 = (ncons nil)
)
tmp1->name = "VAM"
tmp1->expression = "2.2"
tmp1->index = 1
(unless tmp2
tmp2 = make_sevVariableStruct()
)
(unless tmp2
tmp2 = (ncons nil)
)
tmp2->name = "TX_DATA"
tmp2->expression = "0"
tmp2->index = 2
list(tmp1 tmp2)
)
